<view class="content data-v-09b55120"><block wx:if="{{!modal}}"><view data-event-opts="{{[['tap',[['showCalendar']]]]}}" class="order-time data-v-09b55120" bindtap="__e"><view class="time-viewer data-v-09b55120"><text class="goInHotel data-v-09b55120">入住</text><text class="date-wrappper data-v-09b55120">{{choiceDate[0].month+"月"+choiceDate[0].day+"日"}}</text><text class="sumCount data-v-09b55120">{{dayCount2}}</text><text class="goInHotel data-v-09b55120">离店</text><text class="date-wrappper data-v-09b55120">{{choiceDate[1].month+"月"+choiceDate[1].day+"日"}}</text></view></view></block><view class="{{['calendar-layer','data-v-09b55120',isShow_H5?'show':'hide']}}" animation="{{animationData}}"><view class="{{['layer-content','data-v-09b55120',(choice===true||singleDate)?'choiceDate':'']}}"><view class="layer-body data-v-09b55120"><view class="week-box data-v-09b55120"><block wx:for="{{weekNameArr}}" wx:for-item="item" wx:for-index="tmpIndex" wx:key="tmpIndex"><text class="week-box-item data-v-09b55120">{{item}}</text></block></view><scroll-view class="layer-list data-v-09b55120" scroll-y="true" enable-flex="{{false}}"><block wx:for="{{$root.l1}}" wx:for-item="monthData" wx:for-index="index" wx:key="index"><view class="month data-v-09b55120"><view class="{{['month-title','data-v-09b55120','m-'+monthData.$orig[0].year+'-'+monthData.$orig[0].month]}}" style="{{('z-index:'+index)}}">{{''+(monthData.$orig[0].year+'年'+monthData.$orig[0].month+'月')+''}}</view><view class="month-content data-v-09b55120"><block wx:for="{{monthData.l0}}" wx:for-item="data" wx:for-index="index2" wx:key="index2"><view class="{{['day','data-v-09b55120',data.$orig.re<today?'disabled':data.$orig.selected?'active'+(data.$orig.date==choiceDate[0].date?' begin':data.$orig.date==choiceDate[1].date?' end':''):'']}}" style="{{(index2==0?'margin-left:'+weeks[index]*(100/7)+'%':'')}}" data-index="{{index}}" data-indexs="{{index2}}" data-event-opts="{{[['tap',[['dayClick',['$event']]]]]}}" bindtap="__e"><view class="day-content data-v-09b55120"><block wx:if="{{data.m0==='假'}}"><text class="day-subject holiday data-v-09b55120">{{data.m1}}</text></block><block wx:else><block wx:if="{{data.m2==='班'}}"><text class="day-subject workday data-v-09b55120">{{data.m3}}</text></block><block wx:else><text class="day-subject data-v-09b55120">{{data.m4}}</text></block></block><text class="day-txt data-v-09b55120">{{data.m5}}</text><block wx:if="{{pageSource}}"><text class="day-price data-v-09b55120">{{data.$orig.act.dingdan?"无房":"￥"+data.$orig.price}}</text></block><block wx:else><text class="day-price data-v-09b55120"></text></block><block wx:if="{{!singleDate}}"><text class="day-tip data-v-09b55120">{{data.$orig.act.tip}}</text></block></view><block wx:if="{{choice===false&&!singleDate}}"><view class="beginTip data-v-09b55120">请选择离店日期</view></block><block wx:if="{{choice}}"><view class="endTip data-v-09b55120">{{dayCount2}}</view></block></view></block></view></view></block></scroll-view></view><view class="layer-footer data-v-09b55120"><block wx:if="{{choice===true||singleDate}}"><view data-event-opts="{{[['tap',[['submitbtn',['$event']]]]]}}" class="submitBtn data-v-09b55120" bindtap="__e">完成</view></block></view></view></view></view>